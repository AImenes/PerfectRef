@prefix : <http://www.co-ode.org/ontologies/pizza/pizza.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pizza: <http://www.co-ode.org/ontologies/pizza/pizza.owl#> .
@base <http://www.co-ode.org/ontologies/pizza/pizza.owl> .

<http://www.co-ode.org/ontologies/pizza/pizza.owl> rdf:type owl:Ontology ;
                                                    rdfs:comment "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
                                                    owl:versionInfo "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en ,
                                                                    "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en ,
                                                                    "version 1.5"^^xsd:string .

#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasBase
pizza:hasBase rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf pizza:hasIngredient ;
              owl:inverseOf pizza:isBaseOf ;
              rdf:type owl:FunctionalProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain pizza:Pizza ;
              rdfs:range pizza:PizzaBase .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin
pizza:hasCountryOfOrigin rdf:type owl:ObjectProperty .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient
pizza:hasIngredient rdf:type owl:ObjectProperty ;
                    owl:inverseOf pizza:isIngredientOf ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:domain pizza:Food ;
                    rdfs:range pizza:Food ;
                    rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness
pizza:hasSpiciness rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:range pizza:Spiciness ;
                   rdfs:comment "A property created to be used with the ValuePartition - Spiciness."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping
pizza:hasTopping rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf pizza:hasIngredient ;
                 owl:inverseOf pizza:isToppingOf ;
                 rdf:type owl:InverseFunctionalProperty ;
                 rdfs:domain pizza:Pizza ;
                 rdfs:range pizza:PizzaTopping ;
                 rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf
pizza:isBaseOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf pizza:isIngredientOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain pizza:PizzaBase ;
               rdfs:range pizza:Pizza .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf
pizza:isIngredientOf rdf:type owl:ObjectProperty ,
                              owl:TransitiveProperty ;
                     rdfs:domain pizza:Food ;
                     rdfs:range pizza:Food ;
                     rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf
pizza:isToppingOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf pizza:isIngredientOf ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain pizza:PizzaTopping ;
                  rdfs:range pizza:Pizza ;
                  rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"@en .


#################################################################
#    Classes
#################################################################

###  http://www.co-ode.org/ontologies/pizza/pizza.owl#American
pizza:American rdf:type owl:Class ;
               rdfs:subClassOf pizza:NamedPizza ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pizza:hasTopping ;
                                 owl:someValuesFrom pizza:MozzarellaTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pizza:hasTopping ;
                                 owl:someValuesFrom pizza:PeperoniSausageTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pizza:hasTopping ;
                                 owl:someValuesFrom pizza:TomatoTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pizza:hasTopping ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:unionOf ( pizza:MozzarellaTopping
                                                                   pizza:PeperoniSausageTopping
                                                                   pizza:TomatoTopping
                                                                 )
                                                   ]
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pizza:hasCountryOfOrigin ;
                                 owl:hasValue pizza:America
                               ] ;
               owl:disjointWith pizza:AmericanHot ,
                                pizza:Cajun ,
                                pizza:Capricciosa ,
                                pizza:Caprina ,
                                pizza:Fiorentina ,
                                pizza:FourSeasons ,
                                pizza:FruttiDiMare ,
                                pizza:Giardiniera ,
                                pizza:LaReine ,
                                pizza:Margherita ,
                                pizza:Mushroom ,
                                pizza:Napoletana ,
                                pizza:Parmense ,
                                pizza:PolloAdAstra ,
                                pizza:PrinceCarlo ,
                                pizza:QuattroFormaggi ,
                                pizza:Rosa ,
                                pizza:Siciliana ,
                                pizza:SloppyGiuseppe ,
                                pizza:Soho ,
                                pizza:UnclosedPizza ,
                                pizza:Veneziana ;
               rdfs:label "Americana"@pt .


